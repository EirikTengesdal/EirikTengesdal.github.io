{%- if site.email -%}
  <a class="fa-stack small" href="mailto:{{ site.email | encode_email }}" title="email">
    <i class="fas fa-square fa-stack-2x"></i>
    <i class="fa-solid fa-envelope fa-stack-1x fa-inverse"></i>
  </a>
{%- endif -%}
{% comment %} {%- if site.work_url -%}
